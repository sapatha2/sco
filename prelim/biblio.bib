file = {:Users/shiveshpathak/Library/Application Support/Mendeley Desktop/Downloaded/Armitage, Fournier, Greene - 2010 - Progress and perspectives on electron-doped cuprates.pdf:pdf},
isbn = {0034-6861},
issn = {00346861},
journal = {Reviews of Modern Physics},
number = {3},
pages = {2421--2487},
title = {{Progress and perspectives on electron-doped cuprates}},
volume = {82},
year = {2010}
}

